server {
    listen       80 default_server;
    listen       443 default_server;
    server_name  _;
    include         /etc/nginx/proxy.conf;
    <tmpl_loop name="nginx_rewrite_rules">
        rewrite ^{tmpl_var name="rewrite_url_src"}  {tmpl_var name="rewrite_url_dst"};
    </tmpl_loop>
}
